{
  "params": {
    "device": "cuda",

    "model_type": "rnn",
    "ensemble_size": 1,
    "big_extension": false,
    "freeze_layers": true,
    "trx_encoder": {
      "embeddings": {
        "trans_date": {"in": 800},
        "small_group": {"in": 250}
      },
      "numeric_values": {
        "amount_rur": "identity"
      }
    },
    "rnn": {
      "hidden_size": 800
    },
    "train": {
      "random_neg": false,
      "trx_dropout": 0.00,
      "max_seq_len": 1200,
      "augmentation":false,
      "SubsamplingDataset": {
        "min_seq_len_s": 30,
        "min_seq_len_e": 300,
        "max_seq_len_s": 120,
        "max_seq_len_e": 700,
        "crop_proba_init": 1.0,
        "crop_proba_gamma": 1.0,
        "total_n_epoch": 15
      },
      "batch_size": 128,
      "num_workers": 16,
      "loss": "kruzh",
      "lr": 0.002,
      "weight_decay": 0.000001,
      "n_epoch": 30
    },
    "lr_scheduler": {
      "ReduceLROnPlateau":true,
      "patience":2,
      "factor":0.5 
    },
    "valid": {
      "max_seq_len": 1200,
      "batch_size": 64,
      "num_workers": 8
    },
    "norm_scores": false,
    "score_metric": "accuracy",

    "pretrained_model_path": "models/mles_model.p",
    "use_batch_norm": true
  },

  "save_model": false,

  "stats": {
    "path": "runs/age_pred_target_results.json"
  },

  "output": {
    "valid": {
      "path": "data/mles_finetuning_scores/valid",
      "format": "pickle"
    },
    "test": {
      "path": "data/mles_finetuning_scores/test",
      "format": "pickle"
    },
    "columns": ["client_id"]
  }
}
