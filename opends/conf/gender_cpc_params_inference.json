{
  "params": {
    "device": "cuda",
    "trx_encoder": {
      "embeddings": {
        "mcc_code": {"in": 200},
        "tr_type": {"in": 100}
      },
      "numeric_values": {
        "amount": "identity"
      }
    },
    "norm_scores": false,
    "valid": {
      "batch_size": 64,
      "num_workers": 16,
      "max_seq_len": 1200
    }
  },

  "model_path": {
    "models": [
      "models/gender_cpc_model.p"
    ]
  },

  "output": {
    "path": "../data/gender/embeddings_cpc",
    "format": "pickle",
    "columns": ["customer_id"]
  }
}
